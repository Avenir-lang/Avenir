pckg std.net;

// Satisfies file-to-struct mapping for server.av.
struct server {}

pub struct Server {
    handle | any
}

pub fun (s | Server).accept() | Socket {
    var h | any = __builtin_socket_accept(s.handle);
    return Socket{handle = h};
}

pub fun (s | Server).close() | void {
    __builtin_socket_close(s.handle);
}
